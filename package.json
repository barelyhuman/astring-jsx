{
  "name": "@barelyhuman/astring-jsx",
  "version": "2.0.3",
  "description": "astring, with JSX support",
  "homepage": "https://github.com/barelyhuman/astring-jsx#readme",
  "bugs": {
    "url": "https://github.com/barelyhuman/astring-jsx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barelyhuman/astring-jsx.git"
  },
  "license": "MIT",
  "author": "Stephen Belanger <admin@stephenbelanger.com>",
  "contributors": [
    "reaper <ahoy@barelyhuman.dev>"
  ],
  "type": "commonjs",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "default": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "fix": "npx prettier --write .",
    "next": "bumpp",
    "build": "tsup",
    "test": "vitest ./tests/*",
    "test:ci": "vitest run --coverage"
  },
  "prettier": "@barelyhuman/prettier-config",
  "dependencies": {
    "astring": "^1.8.6",
    "xtend": "^4.0.1"
  },
  "files": [
    "./src",
    "./dist"
  ],
  "devDependencies": {
    "@barelyhuman/node-snapshot": "^1.0.2",
    "@barelyhuman/prettier-config": "^1.1.0",
    "@types/node": "^20.17.32",
    "@vitest/coverage-v8": "3.1.2",
    "acorn": "^8.10.0",
    "acorn-jsx": "^5.3.2",
    "bumpp": "^9.1.1",
    "prettier": "^3.0.1",
    "publint": "^0.3.12",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "astring": "^1.8.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
